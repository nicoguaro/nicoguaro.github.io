&lt;a name="rest_code_6730e4672c934b45ac40baa36b7baaba-4"&gt;&lt;/a&gt;&lt;span class="kn"&gt;import&lt;/span&gt; &lt;span class="nn"&gt;matplotlib.pyplot&lt;/span&gt; &lt;span class="kn"&gt;as&lt;/span&gt; &lt;span class="nn"&gt;plt&lt;/span&gt;
&lt;a name="rest_code_6730e4672c934b45ac40baa36b7baaba-5"&gt;&lt;/a&gt;
&lt;a name="rest_code_6730e4672c934b45ac40baa36b7baaba-6"&gt;&lt;/a&gt;
