&lt;a id="rest_code_3fc9a710216447f8975b397657638279-10" name="rest_code_3fc9a710216447f8975b397657638279-10"&gt;&lt;/a&gt;    &lt;span class="n"&gt;y&lt;/span&gt;&lt;span class="p"&gt;[:,&lt;/span&gt; &lt;span class="mi"&gt;0&lt;/span&gt;&lt;span class="p"&gt;]&lt;/span&gt; &lt;span class="o"&gt;=&lt;/span&gt; &lt;span class="n"&gt;y0&lt;/span&gt;
&lt;a id="rest_code_3fc9a710216447f8975b397657638279-11" name="rest_code_3fc9a710216447f8975b397657638279-11"&gt;&lt;/a&gt;    &lt;span class="k"&gt;for&lt;/span&gt; &lt;span class="n"&gt;cont&lt;/span&gt; &lt;span class="ow"&gt;in&lt;/span&gt; &lt;span class="nb"&gt;range&lt;/span&gt;&lt;span class="p"&gt;(&lt;/span&gt;&lt;span class="mi"&gt;1&lt;/span&gt;&lt;span class="p"&gt;,&lt;/span&gt; &lt;span class="n"&gt;ntimes&lt;/span&gt;&lt;span class="p"&gt;):&lt;/span&gt;
