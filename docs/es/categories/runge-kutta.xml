&lt;a id="rest_code_3fc9a710216447f8975b397657638279-24" name="rest_code_3fc9a710216447f8975b397657638279-24"&gt;&lt;/a&gt;    &lt;span class="k"&gt;return&lt;/span&gt; &lt;span class="n"&gt;np&lt;/span&gt;&lt;span class="o"&gt;.&lt;/span&gt;&lt;span class="n"&gt;array&lt;/span&gt;&lt;span class="p"&gt;(&lt;/span&gt;&lt;span class="n"&gt;dydt&lt;/span&gt;&lt;span class="p"&gt;)&lt;/span&gt;
&lt;a id="rest_code_3fc9a710216447f8975b397657638279-25" name="rest_code_3fc9a710216447f8975b397657638279-25"&gt;&lt;/a&gt;
&lt;a id="rest_code_3fc9a710216447f8975b397657638279-26" name="rest_code_3fc9a710216447f8975b397657638279-26"&gt;&lt;/a&gt;
&lt;a id="rest_code_3fc9a710216447f8975b397657638279-27" name="rest_code_3fc9a710216447f8975b397657638279-27"&gt;&lt;/a&gt;&lt;span class="n"&gt;b&lt;/span&gt; &lt;span class="o"&gt;=&lt;/span&gt; &lt;span class="mf"&gt;0.25&lt;/span&gt;
